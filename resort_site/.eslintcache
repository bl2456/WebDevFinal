ort_site\\src\\pages\\Rooms.js",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\pages\\OneRoom.js",["93"],"import React, { Component } from 'react'\r\nimport HeroContent from '../components/HeroContent';\r\nimport Button from 'react-bootstrap/Button';\r\nimport {Link} from 'react-router-dom';\r\nimport {RoomContext} from '../context';\r\nimport StyledHero from '../components/StyledHero';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Row from 'react-bootstrap/Row';\r\nimport Col from 'react-bootstrap/Col';\r\nimport Carousel from 'react-bootstrap/Carousel';\r\n\r\nexport default class OneRoom extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        //console.log(this.props);\r\n        this.state ={\r\n            variable: this.props.match.params.RoomName,\r\n        }\r\n    }\r\n\r\n    static contextType = RoomContext;\r\n    render() {\r\n        const {getRoom} = this.context;\r\n        const this_room = getRoom(this.state.variable);\r\n        console.log(this_room)\r\n\r\n        const {name, description, capacity, size, price, extras, breakfast, beds, bathrooms, pets, images} = this_room;\r\n\r\n        return (\r\n            <>\r\n                <StyledHero img={images[0]}>\r\n                    <HeroContent title={`${name} Room`}>\r\n                        <Link to='/Rooms'>\r\n                                <Button className='my-btn' variant=\"light\" size=\"lg\">Return to Rooms</Button>\r\n                        </Link>\r\n                    </HeroContent>\r\n                </StyledHero>\r\n                <Container>\r\n                    <div className='carousel-container'>\r\n                        <Carousel>\r\n                            {images.map((image, index) => {\r\n                                return(\r\n                                    <Carousel.Item key={index}>\r\n                                        <img\r\n                                            className=\"d-block w-100\"\r\n                                            src={image}\r\n                                            alt={`${name} image ${index}`}\r\n                                        />\r\n                                    </Carousel.Item>\r\n                                )\r\n                            })}\r\n                        </Carousel>\r\n                    </div>\r\n                </Container>\r\n                <Container className='room-container'>\r\n                    <Row>\r\n                        <Col sm={6}>\r\n                            <div className=\"room-details\">\r\n                                <h3 className='room-section-header'>Description</h3>\r\n                                <p className='room-description'>{description}</p>\r\n                            </div>\r\n                        </Col>\r\n\r\n                        <Col sm={6}>\r\n                            <div className=\"room-info\">\r\n                                <h3 className='room-section-header'>Info</h3>\r\n                                <h5>Price: ${price}</h5>\r\n                                <h5>Size: {size} Squared Feet</h5>\r\n                                <h5>Max Capacity: {(capacity > 1) ? `${capacity} People` : '1 Person'}</h5>\r\n                                <h5>Beds: {beds}</h5>\r\n                                <h5>Bathrooms: {bathrooms}</h5>\r\n                                <h5>Pets: {pets ? \"Allow\" : \"Not Allowed\"}</h5>\r\n                                <h5>Breakfast: {breakfast ? \"Included\" : \"Not Included\"}</h5>\r\n                            </div>\r\n                        </Col>\r\n                    </Row>\r\n                    <Row>\r\n                        <Col>\r\n                            <div className=\"room-extras\">\r\n                                <h3 className='room-section-header'>Extras</h3>\r\n                                <ul className=\"extras\">\r\n                                    <Container>\r\n                                        <Row>\r\n                                            {extras.map((item, index) => {\r\n                                                return(\r\n                                                    <Col md={4} sm={6}>\r\n                                                        <li key={index}>{item}</li>\r\n                                                    </Col>\r\n                                                )\r\n                                            })}\r\n                                        </Row>\r\n                                    </Container>\r\n                                </ul>\r\n                            </div>\r\n                        </Col>\r\n                    </Row>\r\n                </Container>\r\n            </>\r\n        )\r\n    }\r\n}\r\n","C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\pages\\NotFound.js",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\Navbar.jsx",["94"],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\HeroSection.jsx",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\HeroContent.jsx",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\Services.jsx",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\Title.jsx",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\context.js",[],"C:\\Users\\Brian Lu\\Documents\\Web_Dev\\WebDevFinal\\resort_site\\src\\components\\FeaturedRooms.jsx",["95"],"import React, { Component } from 'react';\r\nimport {RoomContext} from '../context';\r\nimport Title from './Title';\r\nimport Room from './Room';\r\n\r\nimport Container from 'react-bootstrap/Container';\r\nimport Row from 'react-bootstrap/Row';\r\nimport Col from 'react-bootstrap/Col';\r\n\r\nexport def